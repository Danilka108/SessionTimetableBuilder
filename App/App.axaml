<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:App"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             x:Class="App.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Resources>
        <ResourceDictionary>
            <system:String x:Key="WorkScheduleString">График работы</system:String>
            <system:String x:Key="TeachersString">Преподаватели</system:String>
            <system:String x:Key="GroupsString">Группы</system:String>
            <system:String x:Key="ClassString">пара</system:String>
            <system:String x:Key="TimeRangeSeparatorString"> - </system:String>
            
                <!-- <Thickness x:Key="MarginSmall">4</Thickness> -->
            <system:Double x:Key="SmallMargin">4</system:Double>
        </ResourceDictionary>
    </Application.Resources> 
    
    <Application.Styles>
        <!-- <StyleInclude Source="avares://Citrus.Avalonia/Rust.xaml"/> -->
        <FluentTheme Mode="Light"></FluentTheme>
        
        <StyleInclude Source="/Styles/Border.axaml"/>
        <StyleInclude Source="/Styles/Font.axaml"/>
    </Application.Styles>
</Application>
